{
    "kind": "DeploymentConfig",
    "apiVersion": "v1",
    "metadata": {
        "name": "goo",
        "creationTimestamp": null,
        "labels": {
            "template": "ivantemplate"
        }
    },
    "spec": {
        "strategy": {
            "type": "Recreate",
            "recreateParams": {
                "post": {
                    "failurePolicy": "Ignore",
                    "execNewPod": {
                        "command": [
                            "/bin/bash",
                            "/init/test/setup.sh"
                        ],
                        "env": [
                            {
                                "name": "POSTGRESQL_USER",
                                "value": "userJPM"
                            },
                            {
                                "name": "POSTGRESQL_PASSWORD",
                                "value": "3D1qciOetqo1do3l"
                            },
                            {
                                "name": "POSTGRESQL_DATABASE",
                                "value": "sampledb"
                            },
                            {
                                "name": "POSTGRESQL_DATABASE",
                                "value": "goo"
                            }
                        ],
                        "containerName": "postgres-postdeploy"
                    }
                }
            },
            "resources": {}
        },
        "triggers": [
            {
                "type": "ImageChange",
                "imageChangeParams": {
                    "automatic": true,
                    "containerNames": [
                        "postgres-postdeploy"
                    ],
                    "from": {
                        "kind": "ImageStreamTag",
                        "namespace": "qa",
                        "name": "postgres-postdeploy:latest"
                    }
                }
            },
            {
                "type": "ConfigChange"
            }
        ],
        "replicas": 1,
        "selector": {
            "name": "goo"
        },
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "name": "goo"
                }
            },
            "spec": {
                "volumes": [
                    {
                        "name": "goo-data",
                        "emptyDir": {}
                    }
                ],
                "containers": [
                    {
                        "name": "postgres-postdeploy",
                        "image": "172.30.167.73:5000/qa/postgres-postdeploy@sha256:5e84ae18ee3bc8f5bf88d6b4ac039a4acb641c2fa7e573a160857269c1a87897",
                        "ports": [
                            {
                                "containerPort": 5432,
                                "protocol": "TCP"
                            }
                        ],
                        "env": [
                            {
                                "name": "POSTGRESQL_USER",
                                "value": "userJPM"
                            },
                            {
                                "name": "POSTGRESQL_PASSWORD",
                                "value": "3D1qciOetqo1do3l"
                            },
                            {
                                "name": "POSTGRESQL_DATABASE",
                                "value": "sampledb"
                            }
                        ],
                        "resources": {},
                        "volumeMounts": [
                            {
                                "name": "goo-data",
                                "mountPath": "/var/lib/pgsql/data"
                            }
                        ],
                        "terminationMessagePath": "/dev/termination-log",
                        "imagePullPolicy": "IfNotPresent",
                        "securityContext": {
                            "capabilities": {},
                            "privileged": false
                        }
                    }
                ],
                "restartPolicy": "Always",
                "terminationGracePeriodSeconds": 30,
                "dnsPolicy": "ClusterFirst",
                "securityContext": {}
            }
        }
    },
    "status": {}
}
